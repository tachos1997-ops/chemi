{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-expressions": "off",
    "no-new": "off",
    "class-methods-use-this": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
